<!DOCTYPE html>
<html lang="fr">

<head>
    <title><%= metaTitle %></title>
    <meta name="description" content="<%= metaDescription %>">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="icon" href="/images/bomb.png" />
    <link rel="stylesheet" href="/stylesheets/dropdown.min.css">
    <link rel="stylesheet" href="/stylesheets/tailwind.min.css" />
    <link rel="stylesheet" href="/stylesheets/extinction.min.css" />
    <link rel="stylesheet" href="/stylesheets/matchstyle.min.css" />
    <link rel="stylesheet" href="/stylesheets/toastr.min.css" />
    <% if (typeof chartist !== 'undefined') { %>
    <link rel="stylesheet" href="//cdn.jsdelivr.net/chartist.js/latest/chartist.min.css">
    <script src="//cdn.jsdelivr.net/chartist.js/latest/chartist.min.js"></script>
    <style>
        .ct-label {
            font-size: 12px;
            color: #c3c3c3;
        }

        .ct-series-a .ct-line {
            /* Set the colour of this series line */
            stroke: #019d9d;
            /* Control the thikness of your lines */
            stroke-width: 3px;
        }

        .ct-series-a .ct-point {
            stroke: #019d9d;
            stroke-width: 8px;
        }

        .ct-grids {
            display: none;
        }
    </style>
    <% } %>
    <script src="/javascripts/navbar.min.js"></script>
</head>

<body class="m-0 p-0 text-white-extinction bg-extinction-dark">
    <%- include navbar.ejs %>